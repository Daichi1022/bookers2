<h1>Users infot</h1>
<%= form_with model: @user do |f| %>
    <label>Name</label>
      <%= f.text_field :name %>
      <label>Image</label>
      <%= f.file_field :profile_image %>
    <label>Intoroducition</label>
      <%= f.text_area :introduction %>
    <%= f.submit "Update User",class: "btn btn-secondary"%>
  <% end %>
 