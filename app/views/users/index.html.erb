<div class="col-md-8 offset-md-1">
      <h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  	 <% @users.each do |user| %>
	  	<tr>
	  	 <td><%#= :profile_image,:fill,50,50,fallback: "no_image.jpg"%></td>
	  	  	  <td><%= user.name %></td>
	  	  	  <td><%#= link_to "Show", %></td>
	  	</tr>
  	  	  <% end %>
  </tbody>
 </table>